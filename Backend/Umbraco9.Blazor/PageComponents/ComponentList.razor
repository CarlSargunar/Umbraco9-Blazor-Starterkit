@using Umbraco9.Core.Models.Elements
@if (Elements == null)
{
    return;
}
@foreach (var el in Elements)
{
    if (el.ModelContentType == HeroElementModel.ModelContentTypeConst)
    {
        var parsed = el as HeroElementModel;
        <HeroElementComponent Model="@parsed" />
    }
}
@code {
    [Parameter]
    public List<IElementModel> Elements { get; set; }
}