@using Umbraco9.Core.Models.Elements
<div style="border: solid red 3px">
    <h2>Title: @Model.Title</h2>
    <p>Subtext: @Model.Subtext</p>
    @if (Model.CallToAction != null)
    {
        <a href="@Model.CallToAction.Url" target="@Model.CallToAction.Target">@Model.CallToAction.Title</a>
    }
</div>

@code {
    [Parameter]
    public HeroElementModel Model { get; set; }
}
