{
  "$schema": "./umbraco/config/appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=(localdb)\\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\\Umbraco.mdf;Integrated Security=True"
  },
  "Umbraco": {
    "CMS": {
      "Hosting": {
        "Debug": false
      },
      "ModelsBuilder": {
        "AcceptUnsafeModelsDirectory": true,
        "ModelsDirectory": "~/../Umbraco9.Core/UmbracoModels",
        "ModelsMode": "SourceCodeManual",
        "ModelsNamespace": "Umbraco9.Core.UmbracoModels"
      },
      "Global": {
        "Id": "35362cde-2452-4c1d-8843-790c201f0ad0"
      }
    }
  },
  "Application": {
    "Version":  "1.0.0.1" 
  }
}